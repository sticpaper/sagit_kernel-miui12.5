config UFS_HOST_PM_QOS_KERNEL
	tristate "Control UFS sleep strategy"
	select SCSI_UFS_QCOM
	help
	  Reduce performance fluctuations.

config PROC_CPUMAXFREQ
	tristate "Export cpumaxfreq"
	help
	  File: /proc/cpumaxfreq.

config SERIAL_NUM
	bool "Enable Serial Number Proc Interface"
	default n
	help
	  Provide a interface for reading CPU serial number

config PAPER_LCD_AUTOSLEEP
	bool "Adjust sleep time"
	default n
	help
	  Enter automatic sleep faster

config KERNEL_KMSG_WRITE
	bool "Allow user space to write logs to the kernel"
	default y
	help
	  If you want to make dmesg only show the kernel log, say N.

config XIAOMI_BINDER_OPT
	bool "critical task opt of binder"
	default n
	help
	  Mark important binder calls from ui thread.

config CHARGE_THERMAL_LIMIT
	bool "Charging temperature upper limit"
	default n
	help
	  Adjust charging temperature control.
